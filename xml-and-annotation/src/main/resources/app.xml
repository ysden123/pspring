<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>
    <!--SQL data processing-->
    <bean id="sqlProcessor2" class="com.stulsoft.pspring.scala.service.Processor" autowire="byType">
        <property name="dataSource" ref="sqlDataSource2"/>
    </bean>

    <bean id="sqlDataSource2" class="com.stulsoft.pspring.scala.data.SqlSource2" autowire="byType"/>

    <!--File data processing-->
    <bean id="fileProcessor2" class="com.stulsoft.pspring.scala.service.Processor" autowire="byType">
        <property name="dataSource" ref="fileDataSource2"/>
    </bean>

    <bean id="fileDataSource2" class="com.stulsoft.pspring.scala.data.FileSource2" autowire="byType"/>

    <bean id="fileDataProvider2" class="com.stulsoft.pspring.scala.data.FileDataProvider2" autowire="byType"/>
    <bean id="sqlDataProvider2" class="com.stulsoft.pspring.scala.data.SqlDataProvider2" autowire="byType"/>
    <bean id="fileSource2" class="com.stulsoft.pspring.scala.data.FileSource2" autowire="byType"/>
    <bean id="sqlSource2" class="com.stulsoft.pspring.scala.data.SqlSource2" autowire="byType"/>
</beans>
